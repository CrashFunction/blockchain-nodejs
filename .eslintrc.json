{
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "plugins": ["node"],
    "extends": ["eslint:recommended", "plugin:node/recommended"],
    "rules": {
        "node/no-unsupported-features": [false],
        "node/exports-style": ["error", "module.exports"]
    }
}
